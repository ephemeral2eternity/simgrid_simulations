# Scenario 0
java -jar nonQoEAgent.jar exp512/platformSce0.xml exp512/deployment.xml
mkdir rsts/nonQoESce0
mv data/* rsts/nonQoESce0/
java -jar nonCoopAgent.jar exp512/platformSce0.xml exp512/deployment.xml
mkdir rsts/nonCoopSce0
mv data/* rsts/nonCoopSce0/
java -jar coopAgent.jar exp512/platformSce0.xml exp512/deployment.xml
mkdir rsts/coopSce0
mv data/* rsts/coopSce0/
echo "Finishing Scenario 0"

